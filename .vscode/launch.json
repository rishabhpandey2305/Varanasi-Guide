{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Varanasi_Guide",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "adb-RZCX904ELBX-UkQmiw._adb-tls-connect._tcp"
        },
        {
            "name": "Varanasi_Guide (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "deviceId": "YOUR_DEVICE_ID"
        },
        {
            "name": "Varanasi_Guide (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "deviceId": "YOUR_DEVICE_ID"
        }
    ]
}
